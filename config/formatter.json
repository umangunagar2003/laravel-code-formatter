{
    "plugins": [
        "@shufo/prettier-plugin-blade",
        "@prettier/plugin-php"
    ],
    "overrides": [
      {
        "files": "*.blade.php",
        "options": {
          "parser": "blade"
        }
      },
      {
        "files": "*.php",
        "options": {
          "parser": "php"
        }
      }
    ],
    "tabWidth": 4,
    "useTabs": false,
    "singleQuote": true,
    "printWidth": 120
  }